{
  "name": "emoji-transmogrifier",
  "description": "a handy little library for converting emoji short codes to html image tags",
  "homepage": "https://edm00se.github.io/emoji-transmogrifier",
  "author": {
    "name": "Eric McCormick",
    "email": "edm00se@gmail.com",
    "url": "https://edm00se.github.com/emoji-transmogrifier"
  },
  "files": [
    "src"
  ],
  "main": "./src/server/index.js",
  "bin": "./src/cli/index.js",
  "keywords": [
    "emoji",
    "image",
    "convert"
  ],
  "devDependencies": {
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "eslint-config-xo-space": "0.7.0",
    "ghooks": "1.0.3",
    "gulp": "3.9.0",
    "gulp-coveralls": "0.1.0",
    "gulp-eslint": "1.0.0",
    "gulp-exclude-gitignore": "1.0.0",
    "gulp-istanbul": "0.10.3",
    "gulp-mocha": "2.0.0",
    "gulp-nsp": "2.1.0",
    "gulp-plumber": "1.0.0",
    "istanbul": "0.4.2",
    "mocha": "2.3.4",
    "semantic-release": "4.3.5"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "mocha": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edm00se/emoji-transmogrifier.git"
  },
  "scripts": {
    "prepublish": "gulp prepublish",
    "test": "gulp && istanbul cover -x test/*.js _mocha -- -R spec test/index.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100 && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js || true"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "2.9.0",
    "fs": "0.0.2",
    "update-notifier": "0.6.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "tonicExampleFilename": "example.js",
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
