{
  "name": "emoji-transmogrifier",
  "description": "a handy little library for converting emoji short codes to html image tags",
  "homepage": "https://edm00se.github.io/emoji-transmogrifier",
  "author": {
    "name": "Eric McCormick",
    "email": "edm00se@gmail.com",
    "url": "https://edm00se.github.com/emoji-transmogrifier"
  },
  "version": "0.0.0-semantically-released",
  "files": [
    "src",
    "example.js"
  ],
  "main": "./src/server/index.js",
  "bin": "./src/cli/index.js",
  "keywords": [
    "emoji",
    "image",
    "convert"
  ],
  "devDependencies": {
    "codecov": "1.0.1",
    "commitizen": "2.8.2",
    "cz-conventional-changelog": "1.1.6",
    "eslint": "3.2.2",
    "eslint-config-xo-space": "0.12.0",
    "eslint-formatter-pretty": "0.2.2",
    "ghooks": "1.3.2",
    "gulp": "3.9.1",
    "gulp-coveralls": "0.1.4",
    "gulp-eslint": "2.0.0",
    "gulp-exclude-gitignore": "1.0.0",
    "gulp-istanbul": "1.0.0",
    "gulp-mocha": "3.0.0",
    "gulp-nsp": "2.4.2",
    "gulp-plumber": "1.1.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.0.0",
    "semantic-release": "6.3.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "es6": true,
      "mocha": true
    },
    "options": {
      "format": "node_modules/eslint-formatter-pretty"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edm00se/emoji-transmogrifier.git"
  },
  "scripts": {
    "prepublish": "gulp prepublish",
    "test": "gulp && istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec && ./node_modules/codecov/bin/codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100 && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js || true"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "2.9.0",
    "glob": "7.0.5",
    "update-notifier": "1.0.2"
  },
  "tonicExampleFilename": "example.js",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  }
}
